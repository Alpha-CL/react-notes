source":null},{"filePath":"49","messages":"50","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"51","messages":"52","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/OldContext/MultiContexts/index.js",[],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/OldContext/SingleContext/index.js",["53"],"///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n\nimport React, {Component} from \"react\";\nimport PropTypes from \"prop-types\";\n\n\n//-------------------------------------------------------------------------------------------------------------------//\n\n\n/** Old Context API\n *\n *\n * ( 只有 class_component 才可以创建 context )\n * ( class_component & func_component 都可以接受数据 )\n **/\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\n/**\n * create context_data\n *\n *\n * 1) 必须创建 staic childContextTypes 约束 context 中传入的数据类型\n * 2) 必须创建 getChildContext(): contextDataObj; 返回的 ContextData 必须满足之前创建的约束类型\n *    ( 该函数修改后会在 render 后运行 )\n */\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\n/**\n * use context_data\n *\n *\n * 必须创建 contextTypes 用于约束指定接受部分数据及其类型\n *\n * 1) 后代类组件的 constructor 的第二个参数，可以获取上下文中的指定数据\n * 2) 后代类组件的 context 属性，可以获取上下文中的指定数据( 需在 super(props, context) 传递 )\n */\n\n\n//-------------------------------------------------------------------------------------------------------------------//\n\n\nconst types = {\n    name: PropTypes.string.isRequired,\n    age: PropTypes.number.isRequired,\n    handleAgeChange: PropTypes.func\n};\n\nfunction Child(props, context) {\n\n    return (\n        <React.Fragment>\n            <h3>Child Context</h3>\n            <p>context.name: {context.name}</p>\n            <p>context.age: {context.age}</p>\n            <hr/>\n            <h3>SubChild Context</h3>\n            <SubChild/>\n            <hr/>\n        </React.Fragment>\n    );\n}\n\n/** 函数组件也必须使用 contextTypes 约束数据传入 **/\nChild.contextTypes = types;\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nclass SubChild extends Component {\n\n    /** 用于约束指定接受部分数据及其类型 **/\n\n    static contextTypes = types;\n\n    /** getContentData-Methods_01 **/\n    /** constructor.param_02: 第二个参数 **/\n    constructor(props, context) {\n\n        super(props, context);\n\n        // console.log('[context]: ', context);\n    }\n\n    render() {\n\n        return (\n            <React.Fragment>\n\n                {/** getContentData-Methods_02 **/}\n                {/* this.context */}\n                <p>context.name: {this.context.name}</p>\n                <p>context.age: {this.context.age}</p>\n                <button\n                    onClick={() => {\n                        this.context.handleAgeChange(this.context.age + 2);\n                    }}\n                >change age in context data\n                </button>\n            </React.Fragment>\n        );\n    }\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nclass SignleContext extends Component {\n\n    /** 约束 context 中的数据类型 **/\n\n    static childContextTypes = types;\n\n    constructor(props) {\n\n        super(props);\n\n        this.state = {\n\n            name: 'alpha',\n            age: 18\n        };\n    }\n\n    /** 获取 context 中的数据 **/\n\n    getChildContext() {\n\n        // console.log('[getChildContext]');\n\n        return {\n\n            name: this.state.name,\n            age: this.state.age,\n            handleAgeChange: (newAge) => {\n                this.setState({\n                    age: newAge\n                });\n            }\n        };\n    }\n\n    render() {\n\n        return (\n\n            <React.Fragment>\n\n                <h1>Single Context</h1>\n                <h3>Old Context</h3>\n                <p>state.name: {this.state.name}</p>\n                <p>state.age: {this.state.age}</p>\n                <button\n                    onClick={() => {\n                        this.setState({\n                            age: this.state.age + 1\n                        })\n                    }}\n                >change context data\n                </button>\n                <hr/>\n                <Child/>\n            </React.Fragment>\n        );\n    }\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nexport {\n\n    SignleContext\n};\n\n\n///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n","/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/OldContext/index.js",[],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/App.js",["54","55"],"///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n\nimport React, {Component} from \"react\";\nimport {OldContext} from \"./components/OldContext\";\nimport {NewContext} from \"./components/NewContext\";\n\n//-------------------------------------------------------------------------------------------------------------------//\n\n\nexport {\n    App\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nclass App extends Component {\n\n\n    constructor(props) {\n\n        super(props);\n\n        this.state = {};\n    }\n\n    render() {\n\n        return (\n\n            <React.Fragment>\n                {/*<OldContext/>*/}\n                <NewContext/>\n            </React.Fragment>\n        );\n    }\n}\n\n\n///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n","/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/index.js",[],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/Provider/index.js",["56","57"],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/Consumer/index.js",["58","59"],"///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n\nimport React, {Component} from \"react\";\n\n\n//-------------------------------------------------------------------------------------------------------------------//\n\n\n/**\n * Consumer                 // consumer\n *\n *\n * 创建一个用于调用指定 上下文的对象( context )\n * 该组件 必须指定 targetContext.Comsumer\n * 该组件的子节点是一个函数\n *\n * <targetContext.Comsumer>\n *\n *     {(value)=>{              // value: ContextData\n *\n *         return (\n *\n *             <chidren/>\n *         );\n *     }\n *\n * <targetContext.Comsumer/>\n */\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\n/** New Context APi **/\n\nconst ctx = React.createContext();\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nfunction Child(props) {\n\n    return (\n        <React.Fragment>\n            <h3>Child Context</h3>\n            <ctx.Consumer>\n                {(val) => {\n                    return (\n                        <React.Fragment>\n                            <p>context.name: {val.name}</p>\n                            <p>context.age: {val.age}</p>\n                        </React.Fragment>\n                    );\n                }}\n            </ctx.Consumer>\n            <hr/>\n            <h3>SubChild Context</h3>\n            <SubChild/>\n            <hr/>\n        </React.Fragment>\n    );\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nclass SubChild extends Component {\n\n    /** 类组件调用必须条件: 仅用于调用单个 Context 的数据 **/\n    // static contextType = ctx;\n\n    constructor(props) {\n\n        super(props);\n    }\n\n    render() {\n\n        return (\n\n            <ctx.Consumer>\n\n                {(val) => {\n\n                    return (\n                        <React.Fragment>\n                            <p>context.name: {val.name}</p>\n                            <p>context.age: {val.age}</p>\n                            <button\n                                onClick={() => {\n                                    val.changeAge(val.age + 2);\n                                }}\n                            >age + 2\n                            </button>\n                        </React.Fragment>\n\n                    );\n                }}\n\n            </ctx.Consumer>\n        );\n    }\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\n/** create context **/\n\nclass Consumer extends Component {\n\n    static propTypes = {};\n\n    static defaultProps = {};\n\n    constructor(props) {\n\n        super(props);\n\n        this.state = {\n\n            name: 'beta',\n            age: 17,\n            changeAge: (newAge) => {\n                this.setState({\n                    age: newAge\n                });\n            }\n        };\n    }\n\n    render() {\n\n        /** 创建必要条件 **/\n        const Provider = ctx.Provider;                  /** 不可改变命名: 改变后会报错 **/\n\n        return (\n\n            <ctx.Provider\n                value={this.state}\n            >\n                <h3>Consumer</h3>\n                <button\n                    onClick={() => {\n                        this.setState({\n                            age: this.state.age + 1\n                        })\n                    }}\n                >age + 1\n                </button>\n                <hr/>\n                <Child/>\n            </ctx.Provider>\n        );\n    }\n}\n\n\n//- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -//\n\n\nexport {\n\n    Consumer\n};\n\n\n///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n","/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/index.js",[],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/MultiConsumers/index.js",["60","61","62"],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/PerformanceOptimization/NoOptimization/index.js",["63","64"],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/PerformanceOptimization/Optimized/index.js",["65","66"],"/Users/lalpha/SynologyDrive/Drive/dlp/dlp-lrn/GitHub/Framework-library-note/03-react/Example/Ex1.3.00/06-context/src/components/NewContext/DataFlow/index.js",["67"],{"ruleId":"68","severity":1,"message":"69","line":85,"column":5,"nodeType":"70","messageId":"71","endLine":90,"endColumn":6},{"ruleId":"72","severity":1,"message":"73","line":5,"column":9,"nodeType":"74","me